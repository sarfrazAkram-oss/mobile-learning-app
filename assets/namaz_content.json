{
  "namaz": [
    {"title": "Subhanak", "text": "سُبْحَانَكَ اللَّهُمَّ وَبِحَمْدِكَ"},
    {"title": "Shahada 1", "text": "أَشْهَدُ أَنْ لَا إِلٰهَ إِلَّا اللَّهُ"},
    {"title": "Shahada 2", "text": "وَأَشْهَدُ أَنَّ مُحَمَّدًا رَسُولُ اللَّهِ"},
    {"title": "Takbir", "text": "اللَّهُ أَكْبَرْ"},
    {"title": "Subhan Rabbi", "text": "سُبْحَانَ رَبِّيَ الْعَظِيمِ"},
    {"title": "Sami Allah", "text": "سَمِعَ اللَّهُ لِمَنْ حَمِدَهُ"},
    {"title": "Rabbana", "text": "رَبَّنَا لَكَ الْحَمْدُ"},
    {"title": "Subhan Al-Ala", "text": "سُبْحَانَ رَبِّيَ الْأَعْلَى"},
    {"title": "At-Tahiyat", "text": "التَّحِيَّاتُ لِلَّهِ وَالصَّلَوَاتُ وَالطَّيِّبَاتُ"},
    {"title": "Salam", "text": "اَلسَّلَامُ عَلَيْكُمْ وَرَحْمَةُ اللَّهِ"}
  ]
}